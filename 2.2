#include <iostream>

using namespace std;

int main()
{
    int i = 1;
    int s = 0;
    while (i < 1000)
    {
       if (i % 4 == 1) {
           cout << i << endl;
       } 
       ++i;
    }
    
    int j = 1;    
    while (j < 1000)
    {
    if (j % 7 == 0) {
           s += j;
       }
       ++j;
    }
    cout << "sum=" << s << endl;
    return 0;
}
